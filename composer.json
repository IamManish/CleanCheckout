{
    "name": "rubic/magento2-module-clean-checkout",
    "description": "The amazing Clean Checkout module for Magento 2.",
    "type": "magento2-module",
    "license": "MIT",
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Rubic\\CleanCheckout\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "7.0.26"
        }
    },
    "require": {
        "geoip2/geoip2": "^2.0",
        "hybridauth/hybridauth" : "^2.10",
        "magento/framework": ">=100.1.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.9.1"
    },
    "repositories": [
        { "type": "composer", "url": "https://repo.magento.com/" }
    ]
}
